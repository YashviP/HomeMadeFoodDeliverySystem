<!--<script src="https://cdn.jsdelivr.net/npm/places.js@1.18.1"></script>-->
<div class="signup_container">
    <h1 class="text-center">Sign Up</h1>
    <hr>
    
    <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name),data: { controller: "places"}, validate: true) do |f| %>
        <%= devise_error_messages! %>


      <div class="form-group">
       <%= f.label :profile_image %> 
       <%= f.file_field :profile_image %>
      </div>
 
       <div class="form-group">
        <%= f.label :first_name %><br />
        <%= f.text_field :first_name, class: 'form-control'  %>
      </div>


      <div class="form-group">
        <%= f.label :middle_name %><br />
        <%= f.text_field :middle_name, class: 'form-control'  %>
      </div>

      <div class="form-group">
        <%= f.label :last_name %><br />
        <%= f.text_field :last_name, class: 'form-control'  %>
      </div>

      <div class="form-group">
        <%= f.label :mobile_number %><br />
        <%= f.number_field :mobile_number, class: 'form-control' %>
      </div>


      <div class="form-group">
        <%= f.label :email %><br />
        <%= f.email_field :email, class: 'form-control'  %>
      </div>

      <div class="form-group">
        <%= f.label :address %><br />
        <%= f.text_field :address, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :city %><br />
        <%= f.text_field :city, class: 'form-control' %>
      </div>

       <div class="form-group">
        <%= f.label :state %><br />
        <%= f.text_field :state, class: 'form-control' %>
      </div>


      <div class="form-group">
        <%= f.label :country %><br />
        <%= f.text_field :country, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :pincode %><br />
        <%= f.number_field :pincode, class: 'form-control' %>
      </div>
       <div class="form-group">
        <%= f.label :password %> 
        <%= f.password_field :password, autocomplete: "off", class: 'form-control'  %>
      </div>

      <div class="form-group">
        <%= f.label :password_confirmation %><br />
        <%= f.password_field :password_confirmation, autocomplete: "off", class: 'form-control'  %>
      </div>
     
      <div class="form-group">
          <h3><%= f.label "Are you Chef?" %> <%= f.check_box :is_chef , :style => "margin: 10px; width: 20px; height: 20px;" %>  </h3>
          
      </div> 

      <div class="form-actions">
        <%= f.submit "Sign up", class: "btn btn-primary btn-block btn-md" %>
      </div>
    <% end %>


    <div class="text-center">
      Already have account? 
      <%= render "devise/shared/links" %>
    </div>
</div>
</div>
