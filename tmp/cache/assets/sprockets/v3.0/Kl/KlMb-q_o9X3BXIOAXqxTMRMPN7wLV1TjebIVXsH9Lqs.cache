I"C(function() {
  $(document).on('turbolinks:load ready', function() {
    var build_select, get_cities, get_states;
    build_select = function(resource, data) {
      resource.empty();
      resource.append('<option></option');
      return $.each(data, function(i, item) {
        resource.append($('<option>', {
          value: i,
          text: item
        }));
      });
    };
    get_states = function(state, obj) {
      return $.post('/full_country_select/get_states/' + obj.val(), function(data) {
        return build_select(state, data);
      });
    };
    get_cities = function(country, city, obj) {
      return $.post('/full_country_select/get_cities/' + country + '/' + obj.val(), function(data) {
        return build_select(city, data);
      });
    };
    $('#country').on("change", function() {
      $('#city').empty();
      return get_states($('#state'), $(this));
    });
    return $('#state').on('change', function() {
      var country;
      country = $('#country').val();
      return get_cities(country, $('#city'), $(this));
    });
  });

}).call(this);
